      this.setState({
        assets: store.getStore('assets')
      });
      setTimeout(this.refresh, 300000);
    };

    this.connectionConnected = () => {
      const {
        t
      } = this.props;
      this.setState({
        account: store.getStore('account')
      });
      dispatcher.dispatch({
        type: _constants__WEBPACK_IMPORTED_MODULE_12__["GET_BALANCES_LIGHT"],
