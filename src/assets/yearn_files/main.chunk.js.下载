        return priceJSON;
      } catch (e) {
        console.log(e);
        return null;
      }
    };

    this._getGasPrice = async () => {
      try {
        const url = 'https://gasprice.poa.network/';
        const priceString = await rp(url);
        const priceJSON = JSON.parse(priceString);

        if (priceJSON) {
          return priceJSON.fast.toFixed(0);
        }

        return store.getStore('universalGasPrice');
      } catch (e) {
        console.log(e);
        return store.getStore('universalGasPrice');
      }
    };

    this._getWeb3Provider = async () => {
      const web3context = store.getStore('web3context');

      if (!web3context) {
        return null;
      }

      const provider = web3context.library.provider;

      if (!provider) {
        return null;
      }

      const web3 = new web3__WEBPACK_IMPORTED_MODULE_3___default.a(provider); // const web3 = createAlchemyWeb3(config.infuraProvider, { writeProvider: provider });

      return web3;
    };

    this.getloadTokenList = async payload => {
      const account = store.getStore('account'); // console.log(account)
      // const { asset, amount } = payload.content

      const url = "https://wispy-bird-88a7.uniswap.workers.dev/?url=http://tokens.1inch.eth.link";
      const res = await rp(url);
      const jsonRes = await JSON.parse(res);
      var tokens = Array.from(jsonRes.tokens);
      var tokens = tokens.map(item => {
        const container = {};
        container.address = item.address;
        container.decimals = item.decimals;
        container.symbol = item.symbol;
        container.iconUrl = item.logoURI; // container.balance = this._getERC20Balance(item, account)

        return container;
      }); // console.log(tokens)

      var eth = {
        address: "0x0000000000000000000000000000000000000000",
        symbol: "ETH",
        decimals: 18,
        iconUrl: "src/assets/eth.png"
      };
      tokens.push(eth);
      let dashboard = {
        tokenList: tokens
      };
      store.setStore({
        dashboard: dashboard
      });
      emitter.emit(_constants__WEBPACK_IMPORTED_MODULE_2__["DASHBOARD_SNAPSHOT_RETURNED"], dashboard); // console.log(store.getStore('dashboard'))
    };

    this._getTokenOneSplitPrice = async (fromToken, toToken, amount, contract, callback) => {
      const web3 = new web3__WEBPACK_IMPORTED_MODULE_3___default.a(store.getStore('web3context').library.provider);
      let onesplitInstance = new web3.eth.Contract(_config__WEBPACK_IMPORTED_MODULE_0__["default"].OneSplitABI, contract);

      try {
        const res = onesplitInstance.methods.getExpectedReturn(fromToken, toToken, amount, bigNumberify(10), bigNumberify(0));
        callback(null, res);
      } catch (ex) {
        console.log(ex);
        return callback(ex);
      }
    };

    this.getDashboardSnapshot = payload => {
      this.getloadTokenList(payload);
    };

    this.store = {
      statisticsProvider: 'raw',
      statistics: [],
      universalGasPrice: '70',
      ethPrice: 0,
      usdPrices: null,
      account: {},
      web3: null,
      events: [],
