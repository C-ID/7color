class Unlock extends react__WEBPACK_IMPORTED_MODULE_0__["Component"] {
  constructor(props) {
    super();

    this.navigateInvest = () => {
      this.props.history.push('/invest');
    };

    this.error = err => {
      this.setState({
        loading: false,
        error: err,
        metamaskLoading: false,
        ledgerLoading: false
      });
    };

    this.connectionConnected = () => {
      if (this.props.closeModal != null) {
        this.props.closeModal();
      }
    };

    this.connectionDisconnected = () => {
      if (this.props.closeModal != null) {
        this.props.closeModal();
      }
    };

    this.metamaskUnlocked = () => {
      this.setState({
        metamaskLoading: false
      });

      if (this.props.closeModal != null) {
        this.props.closeModal();
      }
    };

    this.ledgerUnlocked = () => {
      this.setState({
        ledgerLoading: false
      });

      if (this.props.closeModal != null) {
        this.props.closeModal();
      }
    };

    this.cancelLedger = () => {
      this.setState({
        ledgerLoading: false
      });
    };

    this.cancelMetamask = () => {
      this.setState({
        metamaskLoading: false
      });
    };

    this.state = {
      error: null,
      metamaskLoading: false,
      ledgerLoading: false
    };
  }

  componentWillMount() {
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_8__["CONNECTION_CONNECTED"], this.connectionConnected);
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_8__["CONNECTION_DISCONNECTED"], this.connectionDisconnected);
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_8__["ERROR"], this.error);
  }

  componentWillUnmount() {
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_8__["CONNECTION_CONNECTED"], this.connectionConnected);
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_8__["CONNECTION_DISCONNECTED"], this.connectionDisconnected);
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_8__["ERROR"], this.error);
  }

