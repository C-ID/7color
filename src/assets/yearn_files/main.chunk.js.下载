          columnNumber: 5
        }
      }));
    };

    this._handleTextFieldChange = event => {
      this.setState({
        textFieldValue: event.target.value
      });
    };

    this.selectToken = event => {
      var symbol_ = event.target.textContent;
      var tokenData = {
        symbol: symbol_,
        address: this.tokenBySymbol(symbol_).address,
        decimals: this.tokenBySymbol(symbol_).decimals,
        balance: this.tokenBySymbol(symbol_).balance
      };
      console.log(tokenData); // if (this.state.pendingTokenSelection == "input") {
      //   this.setState({ inputToken: tokenData })
      // } else {

      this.setState({
        outputToken: tokenData
      }); // }
      // this.updateBalances()
      // this.closeTokenSelector()
      // this.loadApproval()
    };

    this.openInputSelector = () => {
      this.setState({
        pendingTokenSelection: "input"
      });
      this.openTokenSelector();
    };

    this.openOutputSelector = () => {
      this.setState({
        pendingTokenSelection: "output"
      });
      this.openTokenSelector();
    };

    this.openTokenSelector = () => {
      if (this.state.availableTokens !== undefined) {
        this.setState({
          showTokenSelector: true
        }); // this.setState({ visibleTokens: this.sortTokens(this.state.availableTokens) })
      }
    };

    this.closeTokenSelector = () => {
      this.setState({
        showTokenSelector: false
      });
    };

    this.tokenBySymbol = symbol_ => {
      return this.state.dashboard.tokenList.filter(token => {
        return token.symbol.toLowerCase() === symbol_.toLowerCase();
      })[0];
    };

    this.filterTokenSearch = event => {
      var value = this.$("#token-selector input").value.toLowerCase();

      if (value) {
        if (value.length > 40 && value.startsWith("0x")) {
          var token = {
            address: value
          };
          var contract = this.getContract(this.config.erc20Abi, value);
          contract.methods.symbol().call().then(symbol => {
            token.symbol = symbol;
            contract.methods.decimals().call().then(decimals => {
              token.decimals = decimals;
              contract.methods.balanceOf(this.state.defaultAddress).call().then(balance => {
                balance = isNaN(balance) ? 0 : balance / 10 ** token.decimals;
                token.balance = this.round(balance, 2);
                this.state.availableTokens.push(token);
                this.update({
                  visibleTokens: [token]
                });
              });
            });
          });
        } else {
          var tokens = this.sortTokens(this.state.availableTokens.filter(function (x) {
            return x.symbol.toLowerCase().includes(value);
          }), value);
          this.update({
            visibleTokens: this.sortTokens(tokens)
          });
        }
      } else {
        this.update({
          visibleTokens: this.sortTokens(this.state.availableTokens)
        });
      }
    };

    this.sortTokens = (tokens, value) => {
      return tokens.sort(function (a, b) {
        if (a.balance > 0 || b.balance > 0) {
          return b.balance - a.balance;
        } else if (value instanceof String) {
          return a.symbol.toLowerCase().startsWith(value) ? -1 : 1;
        } else {
          return a.symbol - b.symbol;
        }
      });
    };

    this.onSelectChange = event => {
      let val = [];
      val[event.target.name] = event.target.value;
      this.setState(val);
      console.log(this.state);
      localStorage.setItem('yearn.finance-dashboard-basedon', event.target.value);
      this.setState({
        loading: true
      });
      dispatcher.dispatch({
        type: _constants__WEBPACK_IMPORTED_MODULE_10__["GET_DASHBOARD_SNAPSHOT"],
        content: {}
      });
    };

    this.growthClicked = () => {
      const {
        growth
      } = this.state;
      let newGrowth = 0;

      switch (growth) {
        case 0:
          newGrowth = 1;
          break;

        case 1:
          newGrowth = 2;
          break;

        case 2:
          newGrowth = 0;
          break;

        default:
          newGrowth = 0;
      }

      this.setState({
        growth: newGrowth
      });
      localStorage.setItem('yearn.finance-dashboard-growth', newGrowth.toString());
    };

