class MySnackbar extends react__WEBPACK_IMPORTED_MODULE_0__["Component"] {
  constructor(...args) {
    super(...args);
    this.state = {
      open: this.props.open
    };

    this.handleClick = () => {
      this.setState({
        open: true
      });
    };

    this.handleClose = (event, reason) => {
      if (reason === 'clickaway') {
        return;
      }

      this.setState({
        open: false
      });
    };
  }

