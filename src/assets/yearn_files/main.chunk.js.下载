    this._getAPY = asset => {
      const {
        basedOn
      } = this.state;

      if (asset && asset.stats) {
        switch (basedOn) {
          case 1:
            return asset.stats.apyOneDaySample;

          case 2:
            return asset.stats.apyThreeDaySample;

          case 3:
            return asset.stats.apyOneWeekSample;

          case 4:
            return asset.stats.apyInceptionSample;

          default:
            return asset.apy;
        }
      } else if (asset.apy) {
        return asset.apy;
      } else {
        return '0.00';
      }
    };

