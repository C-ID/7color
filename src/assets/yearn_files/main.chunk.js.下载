class Asset extends react__WEBPACK_IMPORTED_MODULE_0__["Component"] {
  constructor() {
    super();

    this.investReturned = () => {
      this.setState({
        loading: false,
        amount: ''
      });
    };

    this.redeemReturned = txHash => {
      this.setState({
        loading: false,
        redeemAmount: ''
      });
    };

    this.errorReturned = error => {
      this.setState({
        loading: false
      });
    };

    this.onChange = event => {
      let val = [];
      val[event.target.id] = event.target.value;
      this.setState(val);
    };

    this.inputKeyDown = event => {
      if (event.which === 13) {
        this.onInvest();
      }
    };

    this.onInvest = () => {
      this.setState({
        amountError: false
      });
      const {
        amount
      } = this.state;
      const {
        asset,
        startLoading
      } = this.props;

      if (!amount || isNaN(amount) || amount <= 0 || amount > asset.balance) {
        this.setState({
          amountError: true
        });
        return false;
      }

      this.setState({
        loading: true
      });
      startLoading();
      dispatcher.dispatch({
        type: _constants__WEBPACK_IMPORTED_MODULE_5__["INVEST"],
        content: {
          amount: amount,
          asset: asset
        }
      });
    };

    this.onRedeem = () => {
      this.setState({
        redeemAmountError: false
      });
      const {
        redeemAmount
      } = this.state;
      const {
        asset,
        startLoading
      } = this.props;

      if (!redeemAmount || isNaN(redeemAmount) || redeemAmount <= 0 || redeemAmount > asset.investedBalance) {
        this.setState({
          redeemAmountError: true
        });
        return false;
      }

      this.setState({
        loading: true
      });
      startLoading();
      dispatcher.dispatch({
        type: _constants__WEBPACK_IMPORTED_MODULE_5__["REDEEM"],
        content: {
          amount: redeemAmount,
          asset: asset
        }
      });
    };

    this.setAmount = percent => {
      if (this.state.loading) {
        return;
      }

      const {
        asset
      } = this.props;
      const balance = asset.balance;
      let amount = balance * percent / 100;

      if (percent === 100 && asset.symbol === 'ETH') {
        amount = amount - 0.009;
      }

      amount = Math.floor(amount * 10000) / 10000;
      this.setState({
        amount: amount.toFixed(4)
      });
    };

    this.setRedeemAmount = percent => {
      if (this.state.loading) {
        return;
      }

      const balance = this.props.asset.investedBalance;
      let amount = balance * percent / 100;
      amount = Math.floor(amount * 10000) / 10000;
      this.setState({
        redeemAmount: amount.toFixed(4)
      });
    };

    this.state = {
      amount: '',
      amountError: false,
      redeemAmount: '',
      redeemAmountError: false,
      account: store.getStore('account')
    };
  }

  componentWillMount() {
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_5__["INVEST_RETURNED"], this.investReturned);
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_5__["REDEEM_RETURNED"], this.redeemReturned);
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_5__["ERROR"], this.errorReturned);
  }

  componentWillUnmount() {
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_5__["INVEST_RETURNED"], this.investReturned);
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_5__["REDEEM_RETURNED"], this.redeemReturned);
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_5__["ERROR"], this.errorReturned);
  }

  render() {
    const {
      classes,
      asset,
      t
    } = this.props;
    const {
      account,
      amount,
      amountError,
      redeemAmount,
      redeemAmountError,
      loading
    } = this.state;
    return /*#__PURE__*/react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement("div", {
      className: classes.actionsContainer,
      __self: this,
      __source: {
        fileName: _jsxFileName,
        lineNumber: 151,
        columnNumber: 13
      }
    }, /*#__PURE__*/react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement("div", {
      className: classes.tradeContainer,
      __self: this,
      __source: {
        fileName: _jsxFileName,
        lineNumber: 152,
        columnNumber: 7
      }
    }, !asset.disabled && /*#__PURE__*/react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement("div", {
