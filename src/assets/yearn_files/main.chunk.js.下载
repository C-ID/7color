          snackbarType: 'Info'
        };
        that.setState(snackbarObj);
      });
    };

    this.connectionDisconnected = () => {
      this.setState({
        account: store.getStore('account')
      });
    };

    this.errorReturned = error => {
      const snackbarObj = {
        snackbarMessage: null,
        snackbarType: null
      };
      this.setState(snackbarObj);
      this.setState({
        loading: false
      });
      const that = this;
      setTimeout(() => {
        const snackbarObj = {
          snackbarMessage: error.toString(),
          snackbarType: 'Error'
        };
        that.setState(snackbarObj);
      });
    };

    this.investReturned = txHash => {
      const snackbarObj = {
        snackbarMessage: null,
