          lineNumber: 506,
          columnNumber: 12
        }
      });
    };

    const account = store.getStore('account');
    this.state = {
      account: account,
      assets: store.getStore('assets').filter(asset => asset.curve === true),
      curveContracts: store.getStore('curveContracts'),
      sendAsset: null,
      receiveAsset: null,
      sendAmount: "",
      // receiveAmount: ""
      bestPrice: 0
    };

    if (account && account.address) {
      dispatcher.dispatch({
