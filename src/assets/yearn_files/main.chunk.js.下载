        that.setState(snackbarObj);
      });
    };

    this.handleTabChange = (event, newValue) => {
      this.setState({
        value: newValue
      });
    };

    this.onChange = event => {
      let val = [];
      val[event.target.id] = event.target.checked;
      this.setState(val);
    };

