class Asset extends react__WEBPACK_IMPORTED_MODULE_0__["Component"] {
  constructor() {
    super();

    this.investReturned = () => {
      this.setState({
        loading: false,
        amount: ''
      });
    };

    this.redeemReturned = txHash => {
      this.setState({
        loading: false,
        redeemAmount: ''
      });
    };

    this.errorReturned = error => {
      this.setState({
        loading: false
      });
    };

    this.lender = val => {
      if (val === 0) {
        return 'NONE';
      } else if (val === 1) {
        return 'dYdX';
      } else if (val === 2) {
        return 'Compound';
      } else if (val === 3) {
        return 'Aave';
      } else if (val === 4) {
        return 'Fulcrum';
      }
    };

    this.onChange = event => {
      let val = [];
      val[event.target.id] = event.target.value;
      this.setState(val);
    };

    this.inputKeyDown = event => {
      if (event.which === 13) {
        this.onInvest();
      }
    };

    this.onInvest = () => {
      this.setState({
        amountError: false
      });
      const {
        amount
      } = this.state;
      const {
        asset,
        startLoading
      } = this.props;

      if (!amount || isNaN(amount) || amount <= 0 || amount > asset.balance) {
        this.setState({
          amountError: true
        });
        return false;
      }

      this.setState({
        loading: true
      });
      startLoading();
      dispatcher.dispatch({
        type: _constants__WEBPACK_IMPORTED_MODULE_4__["DONATE"],
        content: {
          amount: amount,
          asset: asset
        }
      });
    };

    this.onRedeem = () => {
      this.setState({
        redeemAmountError: false
      });
      const {
        asset,
        startLoading
      } = this.props;
      this.setState({
        loading: true
      });
      startLoading();
      dispatcher.dispatch({
        type: _constants__WEBPACK_IMPORTED_MODULE_4__["REBALANCE"],
        content: {
          asset: asset
        }
      });
    };

    this.setAmount = percent => {
      if (this.state.loading) {
        return;
      }

      const {
        asset
      } = this.props;
      const balance = asset.balance;
      let amount = balance * percent / 100;

      if (percent === 100) {
        amount = asset.poolValue / 100 * 10 / 365;
      }

      this.setState({
        amount: amount.toFixed(8)
      });
    };

    this.setRedeemAmount = percent => {
      if (this.state.loading) {
        return;
      }

      const balance = this.props.asset.investedBalance;
      let amount = balance * percent / 100;
      this.setState({
        redeemAmount: amount.toFixed(8)
      });
    };

    this.state = {
      amount: '',
      amountError: false,
      account: store.getStore('account')
    };
  }

  componentWillMount() {
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_4__["DONATE_RETURNED"], this.investReturned);
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_4__["REBALANCE_RETURNED"], this.redeemReturned);
    emitter.on(_constants__WEBPACK_IMPORTED_MODULE_4__["ERROR"], this.errorReturned);
  }

  componentWillUnmount() {
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_4__["DONATE_RETURNED"], this.investReturned);
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_4__["REBALANCE_RETURNED"], this.redeemReturned);
    emitter.removeListener(_constants__WEBPACK_IMPORTED_MODULE_4__["ERROR"], this.errorReturned);
  }

  render() {
    const {
      classes,
      asset
    } = this.props;
    const {
      account,
      amount,
      amountError,
      loading
    } = this.state;
